<%= form_with model: exection_status, url: admin_exection_statuses_path, method: :post, local:true do |f| %>
  <div class='form-group'>
    <%= f.label 'ステータス名' %>
    <%= f.text_field :name, class: 'form-control name', placeholder: 'ステータスを入力' %>
　</div>
　<div class="form-group">
    <%= f.submit "追加", class: 'btn btn-success' %>
  </div>
<% end %>